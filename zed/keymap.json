[
  {
    "context": "VimControl && !menu",
    "bindings": {
      "s": null,
      "shift-j": null,
      "shift-k": null,
      "shift-h": ["workspace::SendKeystrokes", "b"],
      "shift-l": ["workspace::SendKeystrokes", "w"],
      "shift-x": "pane::CloseActiveItem",
      "shift-f h": "pane::ActivatePreviousItem",
      "shift-f l": "pane::ActivateNextItem",
      ", s p": "pane::SplitRight",
      ", v s": "pane::SplitDown",
      ", d f": "editor::GoToDefinitionSplit",
      ", c a": "editor::ToggleCodeActions",
      ", r r": "editor::FindAllReferences",
      ", r n": "editor::Rename",
      ", p p": "file_finder::Toggle",
      "f l": "workspace::ActivatePaneRight",
      "f h": "workspace::ActivatePaneLeft",
      "f k": "workspace::ActivatePaneUp",
      "f j": "workspace::ActivatePaneDown"
    }
  },
  {
    "context": "VimControl && Editor",
    "bindings": {
      "shift-u": ["workspace::SendKeystrokes", "ctrl-u"],
      "shift-d": ["workspace::SendKeystrokes", "ctrl-d"]
    }
  },
  {
    "context": "vim_mode == insert",
    "bindings": {
      "j j": "vim::NormalBefore"
    }
  }
]
