{
  "workbench.fontAliasing": "auto",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "commands": [
        "editor.action.indentLines"
      ],
      "before": [
        ">"
      ]
    },
    {
      "commands": [
        "editor.action.outdentLines"
      ],
      "before": [
        "<"
      ]
    },
    {
      "before": [
        "<S-l>"
      ],
      "after": [
        "w"
      ]
    },
    {
      "before": [
        "<S-h>"
      ],
      "after": [
        "b"
      ]
    },
    {
      "before": [
        "<S-d>"
      ],
      "after": [
        "<C-d>"
      ]
    },
    {
      "before": [
        "<S-u>"
      ],
      "after": [
        "<C-u>"
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "commands": [
        ":nohl"
      ],
      "before": [
        "<C-c>"
      ]
    },
    {
      "commands": [
        "workbench.view.scm",
        "git.stage"
      ],
      "before": [
        "g",
        "a",
        "<leader>"
      ]
    },
    {
      "commands": [
        "git.commit"
      ],
      "before": [
        "g",
        "g",
        "<leader>"
      ]
    },
    {
      "commands": [
        "git.openChange"
      ],
      "before": [
        "g",
        "d",
        "<leader>"
      ]
    },
    {
      "commands": [
        "workbench.explorer.fileView.focus"
      ],
      "before": [
        "p",
        "p",
        "<leader>"
      ]
    },
    {
      "commands": [
        "workbench.action.quickOpen"
      ],
      "before": [
        "g",
        "f",
        "<leader>"
      ]
    },
    {
      "commands": [
        "workbench.view.search"
      ],
      "before": [
        "f",
        "f",
        "<leader>"
      ]
    },
    {
      "commands": [
        "editor.action.revealDefinitionAside"
      ],
      "before": [
        "f",
        "d",
        "<leader>"
      ]
    },
    {
      "commands": [
        "workbench.action.previousEditor"
      ],
      "before": [
        "h",
        "<S-f>"
      ]
    },
    {
      "commands": [
        "workbench.action.nextEditor"
      ],
      "before": [
        "l",
        "<S-f>"
      ]
    },
    {
      "commands": [
        "workbench.action.focusNextGroup"
      ],
      "before": [
        "l",
        "f"
      ]
    },
    {
      "commands": [
        "workbench.action.focusPreviousGroup"
      ],
      "before": [
        "h",
        "f"
      ]
    },
    {
      "commands": [
        ":vs"
      ],
      "before": [
        "<S-s>"
      ]
    },
    {
      "commands": [
        "workbench.action.newWindowTab"
      ],
      "before": [
        "<S-t>"
      ]
    },
    {
      "before": [
        "<S-l>"
      ],
      "after": [
        "w"
      ]
    },
    {
      "before": [
        "<S-h>"
      ],
      "after": [
        "b"
      ]
    },
    {
      "before": [
        "<S-d>"
      ],
      "after": [
        "<C-d>"
      ]
    },
    {
      "before": [
        "<S-u>"
      ],
      "after": [
        "<C-u>"
      ]
    },
    {
      "commands": [
        ":q"
      ],
      "before": [
        "<S-x>"
      ]
    }
  ],
  "vim.leader": ",",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<ESC>"
      ]
    }
  ],
  "vim.hlsearch": true,
  "vim.easymotionMarkerBackgroundColor": "purple",
  "vim.easymotionKeys": "hjklasdfgyuiopqwertnmzxcvbHJKLASDFGYUIOPQWERTNMZXCVB",
  "vim.easymotion": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontSize": 10,
  "search.exclude": {
    "**/elm-stuff": true,
    "**/node_modules/**": true,
    "**/.next/**": true
  },
  "go.vetOnSave": "package",
  "go.useLanguageServer": true,
  "go.toolsManagement.autoUpdate": true,
  "go.toolsEnvVars": {
    "GO111MODULE": "on"
  },
  "go.lintOnSave": "off",
  "go.gocodeAutoBuild": false,
  "go.formatTool": "gofmt",
  "go.buildOnSave": "off",
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/elm-stuff/**": true,
    "**/.next/**": true,
    "**/.git/objects/**": true
  },
  "files.maxMemoryForLargeFilesMB": 1024,
  "editor.tabSize": 2,
  "editor.scrollBeyondLastLine": false,
  "editor.renderControlCharacters": true,
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.fontSize": 10.5,
  "editor.fontFamily": "Monaco, Menlo, 'Courier New', monospace",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[go]": {
    "editor.formatOnSave": true
  },
  "[elm]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  }
}
